{
  "name": "@europeana/portal",
  "version": "0.0.6",
  "description": "Europeana Portal",
  "contributors": [
    "Richard Doe <richard.doe@europeana.eu>",
    "Lutz Biedinger <lutz.biedinger@europeana.eu>",
    "Mirjam Verloop <mirjam.verloop@europeana.eu>"
  ],
  "private": true,
  "keywords": [
    "code4lib",
    "incubator"
  ],
  "license": "EUPL-1.2",
  "engines": {
    "node": "^12",
    "npm": "^6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/europeana/incubator-portal-vue-nuxt.git"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "npm run stylelint && eslint --ext .js,.vue --ignore-path .gitignore .",
    "stylelint": "stylelint \"**/*.scss\" \"**/*.vue\" --config .stylelintrc.js",
    "precommit": "npm run lint",
    "storybook": "start-storybook -p 6006 -s ./.storybook/assets",
    "build-storybook": "build-storybook -s ./.storybook/assets",
    "test": "npm run test:unit && npm run test:e2e",
    "test:ci": "npm run test:unit && npm run test:e2e:ci",
    "build:test": "cross-env NODE_ENV=test nuxt build",
    "start:test": "cross-env NODE_ENV=test PORT=1337 node server/index.js",
    "test:unit": "vue-cli-service test:unit --include tests/unit/setup",
    "test:chrome": "cross-env browser=chrome cucumber-js",
    "test:chrome:headless": "cross-env browser=chromeHeadless cucumber-js",
    "test:gecko": "cross-env browser=gecko cucumber-js --no-strict",
    "test:e2e": "concurrently 'npm run build:test && npm run start:test' 'npm run test:chrome -- -p all' --success first --kill-others",
    "test:e2e:ci": "concurrently 'npm run build:test && npm run start:test' 'npm run test:chrome:headless -- -p all' --success first --kill-others"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.4.1",
    "@nuxtjs/dotenv": "^1.3.0",
    "@nuxtjs/google-tag-manager": "^2.2.0",
    "bootstrap-vue": "^2.0.0",
    "contentful": "^7.4.1",
    "cross-env": "^5.2.1",
    "elastic-apm-node": "^2.16.1",
    "express": "^4.16.4",
    "express-ipfilter": "^1.0.1",
    "http-errors": "^1.7.3",
    "lodash": "^4.17.15",
    "morgan": "^1.9.1",
    "nightwatch-accessibility": "^1.8.0",
    "nuxt": "^2.9.2",
    "nuxt-i18n": "^6.1.3",
    "oembed-parser": "^1.3.2",
    "qs": "^6.6.0",
    "vcap_services": "^0.7.0",
    "vue": "^2.6.10",
    "vue-resize-directive": "^1.1.4",
    "vue-server-renderer": "^2.6.8"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/generator": "^7.6.0",
    "@babel/parser": "^7.3.4",
    "@babel/preset-env": "^7.6.0",
    "@babel/runtime": "^7.6.0",
    "@babel/types": "^7.6.1",
    "@storybook/addon-actions": "^5.0.6",
    "@storybook/addon-links": "^5.0.1",
    "@storybook/vue": "^5.0.1",
    "@vue/cli-plugin-babel": "^3.5.1",
    "@vue/cli-plugin-eslint": "^3.5.1",
    "@vue/cli-plugin-unit-mocha": "^3.5.1",
    "@vue/cli-service": "^3.5.3",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-preset-vue": "^2.0.2",
    "chai": "^4.2.0",
    "chai-string": "^1.5.0",
    "chromedriver": "^76.0.0",
    "concurrently": "^4.1.0",
    "cssesc": "^3.0.0",
    "cucumber": "^5.1.0",
    "cucumber-pretty": "^1.5.0",
    "eslint": "^6.3.0",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "geckodriver": "^1.16.2",
    "is-reachable": "^3.0.0",
    "nightwatch": "^1.2.2",
    "nightwatch-api": "^2.1.2",
    "nock": "^10.0.6",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "regenerator-runtime": "0.13.3",
    "sass-loader": "^8.0.0",
    "sinon": "^7.2.7",
    "storybook-vue-router": "^1.0.6",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended": "^2.1.0",
    "svg-url-loader": "^3.0.2",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10"
  }
}
