{
  "name": "@europeana/portal",
  "version": "1.39.1",
  "description": "Europeana Portal",
  "contributors": [
    "Richard Doe <richard.doe@europeana.eu>",
    "Lutz Biedinger <lutz.biedinger@europeana.eu>",
    "Mirjam Verloop <mirjam.verloop@europeana.eu>",
    "Leonie Peters <leonie.peters@europeana.eu>",
    "Alan Sutherland <alan.sutherland@europeana.eu>"
  ],
  "private": true,
  "keywords": [
    "code4lib"
  ],
  "license": "EUPL-1.2",
  "engines": {
    "node": "^12",
    "npm": "^6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/europeana/portal.js.git"
  },
  "scripts": {
    "analyze": "nuxt build --analyze",
    "build": "nuxt build",
    "build-storybook": "build-storybook -s ./.storybook/assets",
    "build:test": "cross-env NODE_ENV=test nuxt build",
    "dev": "nuxt",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "npm run lint -- --fix",
    "start": "nuxt start",
    "start:inspect": "node --inspect node_modules/.bin/nuxt start",
    "storybook": "start-storybook -p 6006 -s ./.storybook/assets",
    "stylelint": "stylelint \"**/*.scss\" \"**/*.vue\" --config .stylelintrc.js",
    "test": "npm run test:unit && npm run test:size && npm run test:e2e",
    "test:e2e": "npm run test:stack:build && docker-compose -f ./tests/docker/docker-compose.yml run --rm nightwatch",
    "test:setup": "docker-compose -f ./tests/docker/docker-compose.yml:rebuild && cd tests/size/ && npm install",
    "test:size": "npm run build && cd tests/size/ && npm run start",
    "test:stack:build": "docker-compose -f ./tests/docker/docker-compose.yml build --parallel",
    "test:stack:down": "docker-compose -f ./tests/docker/docker-compose.yml down",
    "test:stack:rebuild": "docker-compose -f ./tests/docker/docker-compose.yml:down && docker-compose -f ./tests/docker/docker-compose.yml:build",
    "test:stack:start": "docker-compose -f ./tests/docker/docker-compose.yml start app nginx chrome-en chrome-ja chrome-nl",
    "test:stack:stop": "docker-compose -f ./tests/docker/docker-compose.yml stop app nginx chrome-en chrome-ja chrome-nl",
    "test:unit": "vue-cli-service test:unit --include tests/unit/setup",
    "test:visual": "docker-compose -f ./tests/docker/docker-compose.yml build -q nightwatch && docker-compose -f ./tests/docker/docker-compose.yml run --rm --entrypoint \"npm run percy\" nightwatch tests/visual",
    "version": "node bin/version.js && git add .sonarcloud.properties"
  },
  "dependencies": {
    "@elastic/apm-rum-vue": "^1.1.9",
    "@nuxtjs/auth": "^4.9.1",
    "@nuxtjs/axios": "^5.13.1",
    "@nuxtjs/gtm": "^2.4.0",
    "@nuxtjs/robots": "^2.4.2",
    "@vue-a11y/announcer": "^2.1.0",
    "autosuggest-highlight": "^3.1.1",
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "bootstrap-vue": "^2.21.2",
    "cookie-universal-nuxt": "^2.0.18",
    "core-js": "^3.8.1",
    "deep-object-diff": "^1.1.0",
    "deepmerge": "^4.2.2",
    "defu": "^5.0.0",
    "elastic-apm-node": "^3.9.0",
    "express": "^4.17.1",
    "lodash": "^4.17.20",
    "marked": "^0.7.0",
    "morgan": "^1.9.1",
    "nuxt": "^2.14.12",
    "nuxt-google-optimize": "github:europeana/nuxt-google-optimize#evaluated-experiment-IDs",
    "nuxt-i18n": "^6.10.1",
    "qs": "^6.6.0",
    "swiper": "^6.1.2",
    "vue": "^2.6.12",
    "vue-awesome-swiper": "^4.1.1",
    "vue-client-only": "^2.0.0",
    "vue-i18n": "^8.17.3",
    "vue-masonry": "^0.12.0",
    "vue-matomo": "^4.0.0",
    "vue-scrollto": "^2.18.2",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/vue": "^5.3.18",
    "@vue/cli-plugin-eslint": "^3.5.1",
    "@vue/cli-plugin-unit-mocha": "^3.5.1",
    "@vue/cli-service": "^3.5.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "babel-preset-vue": "^2.0.2",
    "camelcase": "^6.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-string": "^1.5.0",
    "contentful-cli": "^1.5.32",
    "cross-env": "^5.2.1",
    "eslint": "^6.3.0",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "nock": "^10.0.6",
    "properties-reader": "^2.2.0",
    "sass": "^1.30.0",
    "sass-loader": "^8.0.0",
    "sinon": "^7.2.7",
    "sinon-chai": "^3.3.0",
    "storybook-vue-router": "^1.0.7",
    "stylelint": "^13.7.1",
    "stylelint-config-recommended": "^3.0.0",
    "svg-url-loader": "^7.1.1"
  }
}
