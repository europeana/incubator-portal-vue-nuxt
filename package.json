{
  "name": "@europeana/portal",
  "version": "1.10.0",
  "private": true,
  "description": "Europeana Portal",
  "scripts": {
    "build": "nuxt build",
    "test:unit": "vue-cli-service test:unit --include tests/unit/setup",
    "test:e2e": "cross-env NODE_ENV=test npm run stack:up:detach && npm run test:chrome -- -p all; exit=$?; npm run stack:down; exit $exit",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "build-storybook": "build-storybook -s ./.storybook/assets",
    "build:test": "cross-env NODE_ENV=test nuxt build",
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "generate": "nuxt generate",
    "lint:fix": "npm run lint -- --fix",
    "size": "npm run build && size-limit",
    "stack:down": "docker-compose -f ./docker/stack/docker-compose.yml down",
    "stack:up": "docker-compose -f ./docker/stack/docker-compose.yml build && docker-compose -f ./docker/stack/docker-compose.yml up",
    "stack:up:detach": "docker-compose -f ./docker/stack/docker-compose.yml build && docker-compose -f ./docker/stack/docker-compose.yml up -d",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "start:cluster": "cross-env NODE_ENV=production cservice --restartOnMemUsage 268435456 --cli false server/index.js",
    "start:test": "cross-env NODE_ENV=test PORT=1337 node server/index.js",
    "storybook": "start-storybook -p 6006 -s ./.storybook/assets",
    "stylelint": "stylelint \"**/*.scss\" \"**/*.vue\" --config .stylelintrc.js",
    "test": "npm run test:unit && npm run test:e2e",
    "test:chrome": "cross-env browser=chrome cucumber-js",
    "test:chrome:headless": "cross-env browser=chromeHeadless cucumber-js",
    "test:e2e:ci": "cross-env NODE_ENV=test npm run stack:up:detach && npm run test:chrome:headless -- -p all; exit=$?; npm run stack:down; exit $exit",
    "test:e2e:ci:percy": "cross-env NODE_ENV=test npm run stack:up:detach && npm run test:chrome:headless -- -p all; exit=$?; npm run test:percy; npm run stack:down; exit $exit",
    "test:gecko": "cross-env browser=gecko cucumber-js --no-strict",
    "test:percy": "cross-env percy exec -- cucumber-js -p percy;"
  },
