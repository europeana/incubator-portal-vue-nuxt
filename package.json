{
  "name": "@europeana/portal",
  "version": "0.0.1",
  "description": "Europeana Portal",
  "contributors": [
    "Richard Doe <richard.doe@europeana.eu>",
    "Lutz Biedinger <lutz.biedinger@europeana.eu>",
    "Mirjam Verloop <mirjam.verloop@europeana.eu>"
  ],
  "private": true,
  "keywords": [
    "code4lib",
    "incubator"
  ],
  "license": "EUPL-1.1",
  "engines": {
    "node": "^11",
    "npm": "^6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/europeana/incubator-portal-vue-nuxt.git"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "npm run stylelint && eslint --ext .js,.vue --ignore-path .gitignore .",
    "stylelint": "stylelint .scss,.vue --config .stylelintrc.js",
    "precommit": "npm run lint",
    "storybook": "start-storybook -p 6006 -s ./.storybook/assets",
    "build-storybook": "build-storybook -s ./.storybook/assets",
    "test": "npm run test:unit && npm run test:e2e",
    "test:ci": "npm run test:unit && npm run test:e2e:ci",
    "build:test": "cross-env NODE_ENV=test nuxt build",
    "start:test": "cross-env NODE_ENV=test PORT=1337 node server/index.js",
    "test:unit": "vue-cli-service test:unit --include tests/unit/setup",
    "test:chrome": "cross-env browser=chrome cucumber-js",
    "test:chrome:headless": "cross-env browser=chromeHeadless cucumber-js",
    "test:gecko": "cross-env browser=gecko cucumber-js",
    "test:e2e": "concurrently 'npm run build:test && npm run start:test' 'npm run test:chrome -- -p all && npm run test:gecko -- -p all' --success first --kill-others",
    "test:e2e:ci": "concurrently 'npm run build:test && npm run start:test' 'npm run test:chrome:headless -- -p all' --success first --kill-others"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.4.1",
    "@nuxtjs/dotenv": "^1.3.0",
    "bootstrap-vue": "^2.0.0-rc.14",
    "contentful": "^7.4.1",
    "cross-env": "^5.2.0",
    "elastic-apm-node": "^2.6.0",
    "express": "^4.16.4",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "nuxt": "^2.4.5",
    "qs": "^6.6.0",
    "vcap_services": "^0.6.0",
    "vue": "^2.6.8",
    "vue-resize-directive": "^1.1.4",
    "vue-server-renderer": "^2.6.8",
    "vue-video-player": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/generator": "^7.3.4",
    "@babel/parser": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/runtime": "^7.3.4",
    "@babel/types": "^7.3.4",
    "@storybook/addon-actions": "^5.0.1",
    "@storybook/addon-links": "^5.0.1",
    "@storybook/vue": "^5.0.1",
    "@vue/cli-plugin-babel": "^3.5.1",
    "@vue/cli-plugin-eslint": "^3.5.1",
    "@vue/cli-plugin-unit-mocha": "^3.5.1",
    "@vue/cli-service": "^3.5.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-preset-vue": "^2.0.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chromedriver": "^2.46.0",
    "concurrently": "^4.1.0",
    "cssesc": "^3.0.0",
    "cucumber": "^5.1.0",
    "cucumber-pretty": "^1.5.0",
    "eslint": "^5.15.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-vue": "^5.2.2",
    "geckodriver": "^1.16.0",
    "is-reachable": "^3.0.0",
    "nightwatch": "^1.0.19",
    "nightwatch-api": "^2.1.2",
    "nock": "^10.0.6",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "regenerator-runtime": "0.12.1",
    "sass-loader": "^7.1.0",
    "sinon": "^7.2.7",
    "storybook-vue-router": "^1.0.2",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.8"
  }
}
