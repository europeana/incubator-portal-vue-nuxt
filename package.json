{
  "name": "@europeana/portal",
  "version": "1.14.0",
  "description": "Europeana Portal",
  "contributors": [
    "Richard Doe <richard.doe@europeana.eu>",
    "Lutz Biedinger <lutz.biedinger@europeana.eu>",
    "Mirjam Verloop <mirjam.verloop@europeana.eu>"
  ],
  "private": true,
  "keywords": [
    "code4lib",
    "incubator"
  ],
  "license": "EUPL-1.2",
  "engines": {
    "node": "^12",
    "npm": "^6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/europeana/incubator-portal-vue-nuxt.git"
  },
  "scripts": {
    "build": "nuxt build",
    "build-storybook": "build-storybook -s ./.storybook/assets",
    "build:test": "cross-env NODE_ENV=test nuxt build",
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "npm run lint -- --fix",
    "size": "npm run build && size-limit",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "start:cluster": "cross-env NODE_ENV=production cservice --restartOnMemUsage 268435456 --cli false server/index.js",
    "start:test": "cross-env NODE_ENV=test PORT=1337 node server/index.js",
    "storybook": "start-storybook -p 6006 -s ./.storybook/assets",
    "stylelint": "stylelint \"**/*.scss\" \"**/*.vue\" --config .stylelintrc.js",
    "test": "npm run test:unit && npm run size && npm run test:e2e",
    "test:e2e": "docker-compose -f ./docker/stack/docker-compose.yml build -q nightwatch && docker-compose -f ./docker/stack/docker-compose.yml run --rm nightwatch",
    "test:e2e:percy": "cross-env NODE_ENV=test npm run stack:up:detach && npm run test:chrome:headless -- -p all; exit=$?; npm run test:percy; npm run stack:down; exit $exit",
    "test:percy": "cross-env percy exec -- cucumber-js -p percy;",
    "test:stack:build": "docker-compose -f ./docker/stack/docker-compose.yml build --parallel app nginx",
    "test:stack:down": "docker-compose -f ./docker/stack/docker-compose.yml down",
    "test:stack:pull": "docker-compose -f ./docker/stack/docker-compose.yml pull chrome-en chrome-ja chrome-nl",
    "test:stack:rebuild": "npm run test:stack:down && npm run test:stack:build",
    "test:stack:start": "docker-compose -f ./docker/stack/docker-compose.yml start app nginx chrome-en chrome-ja chrome-nl",
    "test:stack:stop": "docker-compose -f ./docker/stack/docker-compose.yml stop app nginx chrome-en chrome-ja chrome-nl",
    "test:unit": "vue-cli-service test:unit --include tests/unit/setup"
  },
  "dependencies": {
    "@elastic/apm-rum": "^4.7.0",
    "@nuxtjs/auth": "^4.9.1",
    "@nuxtjs/axios": "^5.11.0",
    "@nuxtjs/dotenv": "^1.3.0",
    "@nuxtjs/gtm": "^2.2.3",
    "autosuggest-highlight": "^3.1.1",
    "axios": "^0.20.0",
    "bootstrap-vue": "^2.0.0",
    "cluster-service": "^2.1.4",
    "cookie-universal-nuxt": "^2.0.18",
    "cross-env": "^5.2.1",
    "deep-object-diff": "^1.1.0",
    "deepmerge": "^4.2.2",
    "elastic-apm-node": "^3.6.1",
    "express": "^4.16.4",
    "express-ipfilter": "^1.0.1",
    "http-auth": "^3.2.3",
    "lodash": "^4.17.20",
    "marked": "^0.7.0",
    "md5": "^2.2.1",
    "morgan": "^1.9.1",
    "nuxt": "^2.14.6",
    "nuxt-i18n": "^6.10.1",
    "qs": "^6.6.0",
    "swiper": "^6.1.2",
    "vue": "^2.6.12",
    "vue-awesome-swiper": "^4.1.1",
    "vue-client-only": "^2.0.0",
    "vue-i18n": "^8.17.3",
    "vue-scrollto": "^2.18.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/generator": "^7.6.0",
    "@babel/parser": "^7.3.4",
    "@babel/preset-env": "^7.6.0",
    "@babel/runtime": "^7.6.0",
    "@babel/types": "^7.6.1",
    "@size-limit/preset-app": "^4.6.0",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/vue": "^5.3.18",
    "@vue/cli-plugin-babel": "^3.5.1",
    "@vue/cli-plugin-eslint": "^3.5.1",
    "@vue/cli-plugin-unit-mocha": "^3.5.1",
    "@vue/cli-service": "^3.5.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-preset-vue": "^2.0.2",
    "camelcase": "^6.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-string": "^1.5.0",
    "contentful-cli": "^1.3.35",
    "cssesc": "^3.0.0",
    "eslint": "^6.3.0",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "glob": "^7.1.6",
    "nock": "^10.0.6",
    "node-sass": "^4.14.1",
    "nodemon": "^1.18.10",
    "regenerator-runtime": "0.13.3",
    "sass-loader": "^8.0.0",
    "sinon": "^7.2.7",
    "sinon-chai": "^3.3.0",
    "size-limit": "^4.6.0",
    "storybook-vue-router": "^1.0.7",
    "stylelint": "^13.7.1",
    "stylelint-config-recommended": "^3.0.0",
    "svg-url-loader": "^3.0.3",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.12"
  }
}
