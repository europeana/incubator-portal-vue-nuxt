{
  "name": "@europeana/portal",
  "version": "0.0.1",
  "description": "Europeana Portal",
  "contributors": [
    "Richard Doe <richard.doe@europeana.eu>",
    "Lutz Biedinger <lutz.biedinger@europeana.eu>",
    "Mirjam Verloop <mirjam.verloop@europeana.eu>"
  ],
  "private": true,
  "keywords": [
    "code4lib",
    "incubator"
  ],
  "license": "EUPL-1.1",
  "engines": {
    "node": "^11",
    "npm": "^6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/europeana/incubator-portal-vue-nuxt.git"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "npm run stylelint && eslint --ext .js,.vue --ignore-path .gitignore .",
    "stylelint": "stylelint .scss,.vue --config .stylelintrc.js",
    "precommit": "npm run lint",
    "storybook": "start-storybook -p 6006 -s ./.storybook/assets",
    "build-storybook": "build-storybook -s ./.storybook/assets",
    "test": "npm run test:unit && npm run test:e2e",
    "test:ci": "npm run test:unit && npm run test:e2e:ci",
    "build:test": "cross-env NODE_ENV=test nuxt build",
    "start:test": "cross-env NODE_ENV=test PORT=1337 node server/index.js",
    "test:unit": "vue-cli-service test:unit --require tests/unit/setup",
    "test:chrome": "cross-env browser=chrome npm run test:cucumber",
    "test:chrome:headless": "cross-env browser=chromeHeadless npm run test:cucumber",
    "test:gecko": "cross-env browser=gecko npm run test:cucumber",
    "test:cucumber": "cucumber-js tests/features/**/*.feature --require tests/features/config/cucumber.conf.js --require tests/features --format node_modules/cucumber-pretty",
    "test:e2e": "concurrently 'npm run build:test && npm run start:test' 'npm run test:chrome && npm run test:gecko && npm run test:a11y' --success first --kill-others",
    "test:e2e:ci": "concurrently 'npm run build:test && npm run start:test' 'npm run test:chrome:headless && npm run test:a11y' --success first --kill-others",
    "test:a11y": "node tests/a11y/a11y.test.js"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "@nuxtjs/dotenv": "^1.3.0",
    "bootstrap-vue": "^2.0.0-rc.11",
    "contentful": "^7.2.0",
    "cross-env": "^5.2.0",
    "elastic-apm-node": "^2.5.1",
    "express": "^4.16.3",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "nightwatch-accessibility": "^1.8.0",
    "nuxt": "^2.4.2",
    "vcap_services": "^0.6.0",
    "vue": "^2.6.2",
    "vue-resize-directive": "^1.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@storybook/addon-actions": "^4.1.11",
    "@storybook/addon-links": "^4.1.11",
    "@storybook/vue": "^4.1.11",
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-plugin-unit-mocha": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-preset-vue": "^2.0.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chromedriver": "^2.46.0",
    "concurrently": "^4.1.0",
    "cucumber": "^5.1.0",
    "cucumber-pretty": "^1.5.0",
    "eslint": "^5.13.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-vue": "^5.1.0",
    "geckodriver": "^1.15.1",
    "is-reachable": "^3.0.0",
    "nightwatch": "^1.0.19",
    "nightwatch-api": "^2.1.1",
    "nock": "^10.0.6",
    "node-sass": "^4.11.0",
    "nodemon": "^1.11.0",
    "sass-loader": "^7.1.0",
    "sinon": "^7.2.3",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "vue-loader": "^15.6.2",
    "vue-template-compiler": "^2.6.2"
  }
}
